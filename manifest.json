{
	"name": "Site Settings Sidebar",
	"developer": 
	{
		"name" : "Nekomajin",
		"url" : "http://tothrobert.ininet.hu"
	},
	"description" : "__MSG_manifest_description__",
	"default_locale": "en",
	"manifest_version" : 2,
	"version" : "1.0.4",
	"minimum_opera_version" : "30",
	"background" : 
	{
		"scripts" : ["background.js"]
	},
	"sidebar_action": 
	{
		"default_icon": "icons/icon19.png",
		"default_title": "__MSG_manifest_defaultTitle__",
		"default_panel" : "sidebar/sidebar.html"
	},
	"icons" : 
	{
		"128": "icons/icon128.png", 
		"64": "icons/icon64.png", 
		"48": "icons/icon48.png", 
		"38": "icons/icon38.png", 
		"32": "icons/icon32.png", 
		"19": "icons/icon19.png", 
		"16": "icons/icon16.png"
	},
	"options_page" : "options/options.html",
	"web_accessible_resources" : 
	[ 
		"icons/icon128.png"
	],
	"permissions" : 
	[
		"contentSettings",
		"tabs",
		"notifications"
	],
	"content_security_policy": "script-src 'self'; object-src 'self';"
}